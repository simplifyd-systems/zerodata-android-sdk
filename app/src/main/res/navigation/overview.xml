<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/overview"
    app:startDestination="@id/navigation_overview">

    <fragment
        android:id="@+id/navigation_overview"
        android:name="com.simplifydvpn.android.ui.main.overview.OverviewFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard">

        <action
            android:id="@+id/action_navigation_overview_to_navigation_webview"
            app:destination="@id/navigation_webview" />

    </fragment>


    <fragment
        android:id="@+id/navigation_webview"
        android:name="com.simplifydvpn.android.ui.main.webview.WebViewFragment"
        android:label="Webpage"
        tools:layout="@layout/fragment_webview">

        <argument
            android:name="webUrl"
            app:argType="string" />
    </fragment>

</navigation>